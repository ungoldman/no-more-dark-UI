{
  "name": "GitHub UI Fixer",
  "description": "stop it with the bad UI changes",
  "manifest_version": 2,
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": ["github-blanket.js"]
    }
  ],
  "permissions": ["tabs"]
}
